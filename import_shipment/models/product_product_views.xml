<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_normal_form_view_inherit_import_shipment" model="ir.ui.view">
        <field name="name">product.product.form.inherit.import.shipment</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <field name="default_code" position="after">
                <field name="manufacturer_pref"/>
            </field>
        </field>
    </record>
    <record id="product_template_form_view_inherit_import_shipment" model="ir.ui.view">
        <field name="name">product.template.form.inherit.import.shipment</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <field name="default_code" position="after">
                <field name="manufacturer_pref"/>
            </field>
        </field>
    </record>
</odoo>
